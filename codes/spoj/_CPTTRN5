#include<bits/stdc++.h>
using namespace std;
int main()
{
	int t;
	cin>>t;
	while(t--)
	{
		int l,c,s;
		cin>>l>>c>>s;
		for(int i=0;i<=l*(s+1);i++)
		{
			for(int j=0;j<=c*(s+1);j++)
			{
				if(i%(s+1))
				{
					cout<<"*";				
				}
				else
				{
					if(j%(s+1))
					{
						cout<<"*";
					}
					
				}
			}
			cout<<endl;
		}
	}
return 0;
}